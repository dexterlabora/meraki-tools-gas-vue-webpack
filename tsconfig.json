{
  "include": ["./src/**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": { "*": ["types/*"] },
    "target": "es5",
    // "module": "commonjs",
    "outDir": "dist",
    "strictNullChecks": true,
    "noImplicitThis": true,
    "module": "es2015",
    "moduleResolution": "node",
    //"target": "es5",
    "sourceMap": true,
    "allowJs": true,
    "typeRoots": ["./types", "./node_modules/@types"]
  }
}
