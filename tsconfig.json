{
  "include": ["./src/**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": { "*": ["types/*"] },
    "target": "es5",
    // "module": "commonjs",
    "outDir": "dist",
    "lib": [
      // ... other libs
      "ES2021.String"
    ],
    "strictNullChecks": false,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "module": "es2022",
    "moduleResolution": "node",
    //"target": "es5",
    "sourceMap": true,
    "allowJs": true,
    "typeRoots": ["./types", "./node_modules/@types"]
  }
}
